{
  "name": "pre-onboarding-challenge-be-31",
  "version": "4.0.1",
  "description": "CQRS 시스템 설계/구축 챌린지",
  "author": "NarciSource",
  "private": false,
  "license": "ISC",
  "keywords": [
    "wanted",
    "cqrs"
  ],
  "workspaces": [
    "apps/api-server",
    "apps/projection-worker"
  ],
  "scripts": {
    "build": "nest build api && nest build worker",
    "start": "concurrently \"nest start api\" \"nest start worker\"",
    "start:dev": "concurrently \"nest start api --watch\" \"nest start worker --watch\"",
    "start:debug": "concurrently \"nest start api --debug --watch\" \"nest start worker --debug --watch\"",
    "start:prod": "concurrently \"node dist/apps/api-server/main\" \"node dist/apps/projection-worker/main\"",
    "generate-openapi": "npm run generate-openapi -workspace=apps/api-server",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.14.3",
    "mongoose-lean-virtuals": "^2.0.0",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@testcontainers/mongodb": "^10.26.0",
    "@testcontainers/postgresql": "^10.25.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NarciSource/Pre-Onboarding-Challenge-BE-31.git"
  },
  "bugs": {
    "url": "https://github.com/NarciSource/Pre-Onboarding-Challenge-BE-31/issues"
  },
  "homepage": "https://github.com/NarciSource/Pre-Onboarding-Challenge-BE-31#readme"
}
