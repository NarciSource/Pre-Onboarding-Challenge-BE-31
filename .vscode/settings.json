{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".gitignore": ".gitattributes",
    "tsconfig.json": "tsconfig.*.json",
    "*.ts": "$(capture).spec.ts, $(capture).test.ts",
    "*.command.ts": "$(capture).handler.ts",
    "*.query.ts": "$(capture).handler.ts",
    "*.event.ts": "$(capture).handler.ts",
    "package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .prettier*, prettier*, .editorconfig, nest-cli.json",
    "jest.config.ts": "jest.*.ts",
    "docker-compose.yml": "docker-compose.*, Dockerfile.*"
  },
  "cSpell.words": [
    "cpus",
    "debezium",
    "nori",
    "healthcheck",
    "KSQL",
    "KSQLDB"
  ]
}
